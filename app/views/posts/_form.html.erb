<%= form_with model: [@community, @post] do |form| %>
<div class="form-group">
  <%= form.label :title, "Title" %>
  <%= form.text_field :title, class: "form-control"%>

</div>
<div class="form-group">
  <%= form.label :body, "Body" %>
  <%= form.text_area :body, class: "form-control"%>

</div>

<%= form.submit %>
<%end%>